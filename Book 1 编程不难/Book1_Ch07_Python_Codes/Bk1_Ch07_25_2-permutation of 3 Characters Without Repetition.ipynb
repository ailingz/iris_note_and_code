   "source": [
    "import itertools\n",
    "\n",
    "string = 'abc'\n",
    "\n",
    "# 3个不放回取2个的排列\n",
    "perms_2 = itertools.permutations(string, r=2) # r 用于指定返回的排列长度\n",
    "\n",
    "for perm_idx in perms_2:\n",
    "    print(''.join(perm_idx))"
   ]
  }
