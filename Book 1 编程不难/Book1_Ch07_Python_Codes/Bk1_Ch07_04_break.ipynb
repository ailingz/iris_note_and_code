   ],
   "source": [
    "for x in range(10):\n",
    "    if x >= 8:\n",
    "        break\n",
    "    else:\n",
    "        print(x)\n",
    "else: # 当循环被 break 语句打断后，else 语句中的附加操作不会被执行\n",
    "    print('For loop finished')"
   ]
  }
