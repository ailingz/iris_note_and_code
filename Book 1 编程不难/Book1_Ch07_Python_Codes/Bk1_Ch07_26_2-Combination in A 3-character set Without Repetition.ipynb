   "source": [
    "import itertools\n",
    "\n",
    "string = 'abc'\n",
    "\n",
    "# 3个取2个的组合\n",
    "combs_2 = itertools.combinations(string, r=2)\n",
    "\n",
    "print(type(combs_2))\n",
    "\n",
    "for combo_idx in combs_2:\n",
    "    print(''.join(combo_idx))"
   ]
  }
