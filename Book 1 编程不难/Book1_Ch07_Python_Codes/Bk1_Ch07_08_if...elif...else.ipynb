    "num = input('Enter an integer: ')\n",
    "num = int(num)\n",
    "\n",
    "if num > 0:\n",
    "    print('num is positive')\n",
    "elif num < 0:\n",
    "    print('num is negative')\n",
    "else:\n",
    "    print('num is zero')"
   ]
